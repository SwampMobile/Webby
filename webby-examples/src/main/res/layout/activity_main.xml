<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.swampmobile.webby.examples.activities.MainActivity"
    tools:ignore="MergeRootFrame">

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        />

    <Button
        android:id="@+id/button_webby_activity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Webby From Activity"
        />

    <Button
        android:id="@+id/button_webby_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Webby From Fragment"
        />

</LinearLayout>
